{
  "devDependencies": {
    "parcel": "2.9.2",
    "@parcel/transformer-less": "2.9.2",
    "process": "0.11.10"
  },
  "dependencies": {
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-rc.3"
  },
  "scripts": {
    "dev:watch": "yarn parcel --target web",
    "production:build": "yarn parcel build --target frontend --no-cache --no-source-maps"
  },
  "targets": {
    "web": {
      "source": "src/html/index.html",
      "distDir": "dist",
      "scopeHoist": false,
      "includeNodeModules": {
        "storybook": false
      },
      "context": "browser"
    }
  }
}
